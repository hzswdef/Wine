{
    "name": "wine/wine",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "authors": [
        {
            "name": "Valentyn Haletskyi (hzswdef)",
            "homepage": "https://github.com/hzswdef",
            "email": "hzswdef@gmail.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^2.0",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.5",
        "drupal/core-composer-scaffold": "^11.0",
        "drupal/core-project-message": "^11.0",
        "drupal/core-recommended": "^11.0",
        "drupal/gin": "^3.0@rc",
        "drupal/gin_login": "^2.1",
        "drupal/gin_toolbar": "^1.0@rc",
        "drupal/jsonapi_extras": "^3.26",
        "drupal/jsonapi_include": "^1.8",
        "drupal/jsonapi_permission_access": "^1.0",
        "drupal/jsonapi_response_alter": "^2.0",
        "drupal/jsonapi_search_api": "^1.0@RC",
        "drupal/paragraphs": "^1.18",
        "drupal/search_api_solr": "^4.3",
        "drupal/subrequests": "^3.0",
        "drush/drush": "^13.3",
        "mglaman/composer-drupal-lenient": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "mglaman/composer-drupal-lenient": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patches": {},
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/example.gitignore": false,
                "[web-root]/INSTALL.txt": false,
                "[web-root]/README.md": false,
                "[web-root]/robots.txt": "assets/scaffold/robots.txt",
                "[web-root]/sites/README.txt": false,
                "[web-root]/sites/development.services.yml": "assets/scaffold/development.services.yml",
                "[web-root]/modules/README.txt": false,
                "[web-root]/profiles/README.txt": false,
                "[web-root]/themes/README.txt": false
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-lenient": {
            "allowed-list": []
        }
    }
}
